{
  "name": "oiojs",
  "version": "1.0.5",
  "description": "Powerful and flexible browser Http client, focusing on browserï¼Œinspired by Axios and koa-compose",
  "keywords": [
    "xhr",
    "http",
    "ajax",
    "axios",
    "koa-compose",
    "XMLHttpRequest",
    "request",
    "promise",
    "typescript"
  ],
  "author": "fxc <ifxc@qq.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ifxc/oio"
  },
  "bugs": {
    "url": "https://github.com/ifxc/oio/issues"
  },
  "main": "dist/oiojs.umd.js",
  "typings": "dist/typings/index.d.ts",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:main && npm run build:xhr && npm run build:ts",
    "build:main": "vue-cli-service build --no-clean --target lib --name oiojs ./src/index.ts",
    "build:xhr": "vue-cli-service build --no-clean --target lib --name xhr ./src/adapter/xhr.ts",
    "build:ts": "tsc -p tsconfig.type.json",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/mocha": "^5.2.4",
    "@types/qs": "^6.5.3",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-plugin-typescript": "^3.10.0",
    "@vue/cli-plugin-unit-mocha": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
